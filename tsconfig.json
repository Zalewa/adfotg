{
	"compilerOptions": {
		"outDir": "./site/dist/",
		"experimentalDecorators": true,
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "es6",
		"target": "es6",
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"jsxImportSource": "@emotion/react",
		"allowJs": true,
		"moduleResolution": "node"
	}
}
